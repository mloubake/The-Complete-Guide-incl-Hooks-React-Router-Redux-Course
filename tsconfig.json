{
  "compilerOptions": {
    "jsx": "react-jsx",
    "module": "ES2022",
    "plugins": [{ "name": "typescript-plugin-css-modules" }],
    "noImplicitAny": true,
    "removeComments": true,
    "preserveConstEnums": true,
    "sourceMap": true,
    "esModuleInterop": true,
    "lib": ["es6", "dom", "es2021"],
    "target": "ES2021"
  },
  "exclude": ["node_modules"]
}
